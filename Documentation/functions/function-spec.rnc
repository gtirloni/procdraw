element function-spec {
  attribute name { text },
  element syntax { text },
  element description {
    (text
     | element argname { text })+
  },
  element arguments {
    element argument {
      attribute name { text },
      text
    }+
  },
  element examples {
    element example {
      attribute expr { text },
      attribute value { text }
    }+
  }
}
