<function-spec name="keys">

  <syntax>(keys dict)</syntax>

  <description><p>Returns a list of the keys in the dictionary
  <paramname>dict</paramname>.</p></description>

  <parameters>
    <parameter name="dict">A dictionary</parameter>
  </parameters>

  <examples>
    <example expr="(setq d (make-dict))" value="&lt;Dictionary>"/>
    <example expr="(keys d)" value="nil"/>
    <example expr="(put 'key1 42 d)" value="42"/>
    <example expr="(keys d)" value="(key1)"/>
  </examples>

</function-spec>
