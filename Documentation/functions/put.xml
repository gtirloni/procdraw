<function-spec name="put">

  <syntax>(put key value dict)</syntax>

  <description><p>Stores <paramname>value</paramname> as associated to
  the given <paramname>key</paramname> in the dictionary
  <paramname>dict</paramname>. Returns
  <paramname>value</paramname>.</p></description>

  <parameters>
    <parameter name="key">Key of association</parameter>
    <parameter name="value">Value of association</parameter>
    <parameter name="dict">A dictionary</parameter>
  </parameters>

  <examples>
    <example expr="(setq d (make-dict))" value="&lt;Dictionary>"/>
    <example expr="(put 'key1 42 d)" value="42"/>
    <example expr="(put 'key2 10 d)" value="10"/>
    <example expr="(get 'key1 d)" value="42"/>
    <example expr="(get 'key2 d)" value="10"/>
  </examples>

</function-spec>
