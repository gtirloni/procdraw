<function-spec name="get">

  <syntax>(get key dict)</syntax>

  <description><p>Returns the value associated to the given
  <paramname>key</paramname> in the dictionary
  <paramname>dict</paramname>. Returns nil if the key is not
  found.</p></description>

  <parameters>
    <parameter name="key">The key to look for</parameter>
    <parameter name="dict">A dictionary</parameter>
  </parameters>

  <examples>
    <example expr="(setq d (make-dict))" value="&lt;Dictionary>"/>
    <example expr="(put 'key1 42 d)" value="42"/>
    <example expr="(put 'key2 10 d)" value="10"/>
    <example expr="(get 'key1 d)" value="42"/>
    <example expr="(get 'key2 d)" value="10"/>
  </examples>

</function-spec>
