<function-spec name="make-dict">

  <syntax>(make-dict)</syntax>

  <description><p>Makes a new dictionary.</p></description>

  <parameters none="none"/>

  <examples>
    <example expr="(setq d (make-dict))" value="&lt;Dictionary>"/>
    <example expr="(put 'key1 42 d)" value="42"/>
    <example expr="(put 'key2 10 d)" value="10"/>
    <example expr="(get 'key1 d)" value="42"/>
    <example expr="(get 'key2 d)" value="10"/>
  </examples>

</function-spec>
