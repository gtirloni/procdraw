<function-spec name="eq">
  <syntax>(eq x y)</syntax>

  <description>
    <p>Compares objects <paramname>x</paramname> and
    <paramname>y</paramname> for equality. Returns true in the
    following cases:</p>
    <ul>
      <li><paramname>x</paramname> and <paramname>y</paramname> are
      numbers and have the same value</li>
      <li><paramname>x</paramname> and <paramname>y</paramname> are
      strings and have the same contents</li>
      <li><paramname>x</paramname> and <paramname>y</paramname> are
      the same object</li>
    </ul>
    <p>Returns false otherwise.</p>
  </description>

  <parameters>
    <parameter name="x">First object to compare</parameter>
    <parameter name="y">Second object to compare</parameter>
  </parameters>
  <examples>
    <example expr="(eq 42 42)" value="true"/>
    <example expr="(eq 42 1)" value="false"/>
    <example expr="(eq &quot;hello&quot; &quot;hello&quot;)" value="true"/>
    <example expr="(eq &quot;hello&quot; &quot;world&quot;)" value="false"/>
    <example expr="(eq 'foo 'foo)" value="true"/>
    <example expr="(eq '(a b) '(a b))" value="false"/>
    <example expr="(eq nil nil)" value="true"/>
  </examples>
</function-spec>
